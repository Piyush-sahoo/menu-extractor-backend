                "method": "POST",
                "header": [
                    {
                        "key": "Content-Type",
                        "value": "application/json"
                    }
                ],
                "body": {
                    "mode": "raw",
                    "raw": "{\n    \"google_maps_url\": \"https://www.google.com/maps/search/The+Rameshwaram+Cafe+Indiranagar\"\n}"
